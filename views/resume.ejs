        <div class="row">
            <div class="col-md-3 skill">
                <h2>Skills</h2>
                <div class="skill-column">
                    <% skills.forEach(function(skill) { %>
                        <div class="skill-item">
                            <div class="skill-label" href="/resume/<%= skill._id%>">
                                 
                                 <form id="deleteForm" action="/resume/<%= skill._id %>?_method=DELETE" method="POST">
                                     <%= skill.skillName %>
                                     <% if(currentUser) { %>
                                        <button class="btn btn-danger">Delete</button>
                                     <% } %>
                                </form>
                            </div>
                            <div class="skill-icons">
                                <% if(skill.skillRate == "1") { %>
                                    <span class="icon-act"></span>
                                    <span class="icon-non"></span>
                                    <span class="icon-non"></span>
                                    <span class="icon-non"></span>
                                    <span class="icon-non"></span>
                                <% } else if(skill.skillRate == "2") { %>
                                    <span class="icon-act"></span>
                                    <span class="icon-act"></span>
                                    <span class="icon-non"></span>
                                    <span class="icon-non"></span>
                                    <span class="icon-non"></span>
                                <% } else if(skill.skillRate == "3") { %>
                                    <span class="icon-act"></span>
                                    <span class="icon-act"></span>
                                    <span class="icon-act"></span>
                                    <span class="icon-non"></span>
                                    <span class="icon-non"></span>
                                <% } else if(skill.skillRate == "4") { %>
                                    <span class="icon-act"></span>
                                    <span class="icon-act"></span>
                                    <span class="icon-act"></span>
                                    <span class="icon-act"></span>
                                    <span class="icon-non"></span>
                                <% } else { %>
                                    <span class="icon-act"></span>
                                    <span class="icon-act"></span>
                                    <span class="icon-act"></span>
                                    <span class="icon-act"></span>
                                    <span class="icon-act"></span>
                                <% } %>
                                
                            </div>
                        </div>
                    <% }) %>
                    
                    <% if(currentUser) { %>
                        <div class="well">
                            <div class="text-right">
                                <a class="btn btn-primary" role="button" data-toggle="collapse" data-target="#collapseComment1" aria-expanded="false" aria-controls="collapseComment1">
                                <span class="glyphicon glyphicon-plus" aria-hidden="true"></span> Add New Skill </a>
                            </div>
                       
                        
                        <!--Collapse Add a comment form START-->
                        <div class="collapse" id="collapseComment1">
                            <div class="well" style="border-left: 5px solid #00C851; height: 300px;">
                            <!--If the user is logged in, show the new comment form-->
                                <h4> Add a New Skill <span class="glyphicon glyphicon glyphicon-pencil" aria-hidden="true"></span></h4>
                                <form id="add-skill-form" action="/resume" method="POST">
                                    <div class="form-group">
                                        <label>Name of skill</label>
                                        <input name="skill[skillName]" class="form-control" type="text">
                                    </div>
                                    <div class="form-group">
                                        <label>Skill Rate</label>
                                        <select name="skill[skillRate]" class="form-control">
                                            <option value="1">Novice</option>
                                            <option value="2">Advanced Beginner</option>
                                            <option value="3">Competence</option>
                                            <option value="4">Proficient</option>
                                            <option value="5">Expert</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <button class="btn btn-success btn-sm"> Submit <span class="glyphicon glyphicon-skill" aria-hidden="true"></span></button>
                                    </div>
                                </form>
                        
                            </div>
                        </div>
                <!--Collapse Add a comment form END-->
